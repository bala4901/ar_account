<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
		<record id="tree_account_tipeInvoice" model="ir.ui.view">
			<field name="name">Tipe Invoice</field>
			<field name="model">account.invoice.type</field>
			<field name="type">tree</field>
			<field name="arch" type="xml">
				<tree string="Tipe Invoice">
					<field name="kode"/>
					<field name="name"/>
					<field name="property_sequence_invoice"/>
				</tree>
			</field>
		</record>
		
		<record id="form_account_tipeInvoice" model="ir.ui.view">
			<field name="name">Tipe Invoice</field>
			<field name="model">account.invoice.type</field>
			<field name="type">form</field>
			<field name="arch" type="xml">
				<form string="Tipe Invoice">
					<group colspan="4" col="2">
						<group colspan="1" col="2">
							<separator string="Tipe Invoice" colspan="2"/>
							<field name="kode" select="1"/>
							<field name="name" select="1"/>
						</group>
						<group colspan="1" col="2">
							<separator string="Sequence" colspan="2"/>
							<field name="property_sequence_invoice" select="1"/>
						</group>
					</group>
				</form>
			</field>
		</record>

	</data>
</openerp>
