<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>

		<record id="waction_account_otherCashReceipt" model="ir.actions.act_window">
			<field name="name">Other Cash Receipt</field>
			<field name="type">ir.actions.act_window</field>
			<field name="res_model">account.other_cash_receipt</field>
			<field name="search_view_id" ref="select_account_otherCashReceipt"/>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
			<field name="context">{'voucher_type' : 'Other Cash Receipt'}</field>
			<field name="domain">[('voucher_type_id.name','=','Other Cash Receipt')]</field>
		</record>
		
		<record id="wactionView_account_treeOtherCashReceipt" model="ir.actions.act_window.view">
			<field eval="10" name="sequence"/>
			<field name="view_mode">tree</field>
			<field name="view_id" ref="tree_account_otherCashReceipt"/>
			<field name="act_window_id" ref="waction_account_otherCashReceipt"/>
		</record>

		<record id="wactionView_account_formOtherCashReceipt" model="ir.actions.act_window.view">
			<field eval="20" name="sequence"/>
			<field name="view_mode">form</field>
			<field name="view_id" ref="form_account_otherCashReceipt"/>
			<field name="act_window_id" ref="waction_account_otherCashReceipt"/>
		</record>
	</data>
</openerp>
