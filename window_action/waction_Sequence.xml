<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>

		<record id="waction_account_sequence" model="ir.actions.act_window">
			<field name="name">Sequence</field>
			<field name="type">ir.actions.act_window</field>
			<field name="res_model">ir.sequence</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
		</record>
		
		<record id="wactionView_account_treeSequence" model="ir.actions.act_window.view">
			<field eval="10" name="sequence"/>
			<field name="view_mode">tree</field>
			<field name="view_id" ref="tree_base_sequence"/>
			<field name="act_window_id" ref="waction_account_sequence"/>
		</record>

		<record id="wactionView_account_formSequence" model="ir.actions.act_window.view">
			<field eval="20" name="sequence"/>
			<field name="view_mode">form</field>
			<field name="view_id" ref="form_base_sequence"/>
			<field name="act_window_id" ref="waction_account_sequence"/>
		</record>
	</data>
</openerp>
