<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>

		<record id="waction_account_customerInvoice" model="ir.actions.act_window">
			<field name="name">Customer Invoice</field>
			<field name="type">ir.actions.act_window</field>
			<field name="res_model">account.customer_invoice</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
			<field name="search_view_id" ref="search_account_customerInvoice"/>
			<field name="domain">[('invoice_type_id.name','=','Customer Invoice')]</field>
			<field name="context">{'invoice_type' : 'Customer Invoice'}</field>
		</record>
		
		<record id="wactionView_account_treeCustomerInvoice" model="ir.actions.act_window.view">
			<field eval="10" name="sequence"/>
			<field name="view_mode">tree</field>
			<field name="view_id" ref="tree_account_customerInvoice"/>
			<field name="act_window_id" ref="waction_account_customerInvoice"/>
		</record>

		<record id="wactionView_account_formCustomerInvoice" model="ir.actions.act_window.view">
			<field eval="20" name="sequence"/>
			<field name="view_mode">form</field>
			<field name="view_id" ref="form_account_customerInvoice"/>
			<field name="act_window_id" ref="waction_account_customerInvoice"/>
		</record>
	</data>
</openerp>
